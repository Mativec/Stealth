diff --git a/Test.c b/Test.c
new file mode 100644
index 0000000..4fb3bac
--- /dev/null
+++ b/Test.c
@@ -0,0 +1,34 @@
+/**
+ * @file Test.c
+ * @author Matias VECCHIO, Richard ARNAOUT
+ * @brief Implmentation du module de test
+ * @version 0.1
+ * @date 2022-11-14
+ * 
+ * @copyright Copyright (c) 2022
+ * 
+ */
+
+#include "Test.h"
+
+int test_init_case(){
+    int x, y;
+    int result;
+    Case * test;
+
+    x = 5;
+    y = 3;
+
+    test = init_case(x, y);
+    if (test->x == x && test->y == y)
+    {
+        result = 1;
+    }
+    else{
+        fprintf(stderr, "case is null\n");
+        result = 0;
+    }
+
+    free(test);
+    return result;
+}
