+#define __INPUT
+
+#include <MLV/MLV_event.h>
+
+typedef enum __input{
+    NONE,
+    UP,
+    RIGHT,
+    LEFT,
+    DOWN,
+    QUIT
+} Input;
+
+Input get_event();
+
+char *input_to_string(Input event);
+
+#endif
