+        return "Up";
+    
+    case OBJECT_LEFT:
+        return "Left";
+    
+    case OBJECT_DOWN:
+        return "Down";
+    
+    case OBJECT_RIGHT:
+        return "Right";
+    
+    case OBJECT_REVERT:
+        return "Revert";
+    
+    case OBJECT_NONE:
+        return "None";
+    default :
+        return "";
+    }
