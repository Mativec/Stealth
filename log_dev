diff --git a/Object.c b/Object.c
new file mode 100644
index 0000000..76789d2
--- /dev/null
+++ b/Object.c
@@ -0,0 +1,57 @@
+/**
+ * @file Object.c
+ * @author your name (you@domain.com)
+ * @brief
+ * @version 0.1
+ * @date 2022-12-08
+ *
+ * @copyright Copyright (c) 2022
+ *
+ */
+
+#include "Engine/Object.h"
+
+Engine_Obj *init_object(float x, float y, int speed) {
+    Engine_Obj *new;
+
+    new = (Engine_Obj *)malloc(sizeof(Engine_Obj));
+
+    if (new != NULL) {
+        new->x = x;
+        new->y = y;
+        new->speed = speed;
+    }
+    return new;
+}
+
+void move_object(Engine_Obj *obj, Engine_Orientation way) {
+    static int base_x, base_y;
+
+    if (way == OBJECT_REVERT) {
+        obj->x = base_x;
+        obj->y = base_y;
+    } else {
+        base_x = obj->x;
+        base_y = obj->y;
+        switch (way) {
+            case OBJECT_UP:
+                obj->y -= obj->speed;
+                break;
+            case OBJECT_LEFT:
+                obj->x -= obj->speed;
+                break;
+            case OBJECT_RIGHT:
+                obj->x += obj->speed;
+                break;
+            case OBJECT_DOWN:
+                obj->y += obj->speed;
+                break;
+            default:
+                break;
+        }
+    }
+}
+
+char *object_to_string(Engine_Obj obj) {
+    return NULL;
+}
\ No newline at end of file
diff --git a/Player.c b/Player.c
new file mode 100644
index 0000000..5c61513
--- /dev/null
+++ b/Player.c
@@ -0,0 +1,31 @@
+/**
+ * @file Player.c
+ * @author your name (you@domain.com)
+ * @brief
+ * @version 0.1
+ * @date 2022-12-08
+ *
+ * @copyright Copyright (c) 2022
+ *
+ */
+
+#include "Engine/Player.h"
+
+void move_player(Engine_Obj *player, Input way) {
+    switch (way) {
+        case INPUT_UP:
+            move_object(player, OBJECT_UP);
+            break;
+        case INPUT_LEFT:
+            move_object(player, OBJECT_LEFT);
+            break;
+        case INPUT_RIGHT:
+            move_object(player, OBJECT_RIGHT);
+            break;
+        case INPUT_DOWN:
+            move_object(player, OBJECT_DOWN);
+            break;
+        default:
+            break;
+    }
+}
