+int contact_between_objects(Engine_Obj obj1, Engine_Obj obj2){
+    int *coord1, *coord2, result;
+
+    coord1 = get_object_coord(obj1);
+    coord2 = get_object_coord(obj2);
+    
+    result = (coord1[0] == coord2[0]) && (coord1[1] == coord2[1]);
+
+    free(coord1);
+    free(coord2);
+
+    return result;
+}
 
 
