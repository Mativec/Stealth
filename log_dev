+void draw_window(Engine_Obj player, Engine_Obj guard) {
+    static int init = 0;
+
+    /* Window initiated ? */
+    if (!init) {
+        init = 1;
+        init_window();
+    }
+
+    /* clean the window */
+    MLV_clear_window(MLV_COLOR_WHITE);
+
+    /* draw on the window */
+    MLV_draw_filled_circle(player.x, player.y, 10, MLV_COLOR_RED);
+
+    draw_guard(guard);
